
<h1>This is Your Happy Place!!!</h1>
<div id='homepage' class='container'>

  <div id='left_sidebar'>

    <div id='mini_profile'>
        <h3>SNAP SHOT BIO</h3>
        <p>
          <h3><%= @user.firstname %> <%= @user.lastname %></h3>
        </p>
        <p>
          <%= image_tag @user.image_upload.thumb.url unless @user.image_upload.thumb.url.nil? %>
           <%= @user.bio %>
        </p>
    </div><!--SNAP SHOT BIO-->

    <div id='friend_list'>
      <h3>FRIEND LIST</h3>
    </div><!--FRIEND LIST-->
  </div> <!-- End .left_sidebar -->

  <div id='cu_feed_container' class='container'>
    <div id='cu_nav_bar'>
    </div>
    <div id='cu_feed'>
        <%= render partial: 'cheer_ups/single_cu', locals: {cheer_ups: @cheer_ups}  %>
    </div>

  </div><!-- End .cu_feed_container -->
</div>